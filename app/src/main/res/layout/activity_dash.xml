<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/brightboardmenu"
    tools:context="in.bright.avnish.Dash"
    tools:layout_editor_absoluteY="25dp">


    <Button
        android:id="@+id/buttonAddFeeds"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="142dp"
        android:layout_marginStart="142dp"
        android:background="@drawable/cfeedbtn"
        app:layout_constraintBottom_toTopOf="@+id/buttonLogout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="417dp" />

    <Button
        android:id="@+id/buttonLogout"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="@color/blakishBlue"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonFeeds"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="19dp"
        android:layout_marginStart="67dp"
        android:width="250dp"
        android:height="50dp"
        android:background="@drawable/feedbtn"
        app:layout_constraintBottom_toTopOf="@+id/Events"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonNotes"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginBottom="15dp"
        android:layout_marginEnd="81dp"
        android:width="250dp"
        android:height="50dp"
        android:background="@drawable/notesbtn"
        app:layout_constraintBottom_toTopOf="@+id/comm"
        app:layout_constraintEnd_toEndOf="parent" />


    <Button
        android:id="@+id/Events"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginBottom="18dp"
        android:layout_marginStart="67dp"
        android:width="250dp"
        android:height="50dp"
        android:background="@drawable/eventsbtn"
        app:layout_constraintBottom_toTopOf="@+id/buttonAddFeeds"
        app:layout_constraintStart_toStartOf="parent" />


    <Button
        android:id="@+id/comm"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="18dp"
        android:layout_marginEnd="81dp"
        android:width="250dp"
        android:height="50dp"
        android:background="@drawable/combtn"
        app:layout_constraintBottom_toTopOf="@+id/buttonAddFeeds"
        app:layout_constraintEnd_toEndOf="parent" />

</android.support.constraint.ConstraintLayout>
